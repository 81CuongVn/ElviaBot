{
    "name": "Riven",
    "description": "Simple discord bot.",
    "logo": "https://i.ibb.co/CzXcJ0S/photo-2020-01-28-23-32-03.jpg",
    "keywords": [
      "bot"
    ],
    "repository": "https://github.com/XRzky/RivenBot",
    "website": "",
    "success_url": "",
    "stack": "container",
    "env": {
      "TOKEN": {
        "description": "Get this value from developer.discordapp.com.",
        "required": true
      },
      "OWNER": {
        "description": "Input here your ID as the bot owner.",
        "required": true
      },
      "PREFIX": {
        "description": "Input here the prefix for your bot.",
        "value": "?",
        "require": true
      },
      "IMDB_API_KEY": {
        "description": "Get this value from imdb.com.",
        "required": false
      },
      "OPEN_WEATHER_APPID": {
        "description": "Get your own APPID (API key)from https://api.openweathermap.org/data/2.5/weather",
        "required": false
      },
      "TZ": {
        "description": "Input here the time zone location for your localhost. Example: Asia/Jakarta",
        "required": false
      },
      "YOUTUBE_API_KEY": {
        "description": "YouTube Data API Key. Get from https://console.cloud.google.com",
        "required": false
      }
    },
    "addons": [
      {
        "plan": "heroku-postgresql",
        "options": {
          "version": "12"
        }
      }
    ]
}
