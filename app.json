{
    "name": "Elvia",
    "description": "Elvia is a powerful Discord bot made with Node.js with a lot of commands!",
    "logo": "https://i.imgur.com/EDf862b.jpeg",
    "keywords": [
        "discord-bot", "discord.js", "discord-js"
    ],
    "repository": "https://github.com/Rygent/Elvia",
    "website": "",
    "success_url": "https://discord.gg/nW6x9EN",
    "stack": "container",
    "env": {
      "DISCORD_TOKEN": {
        "description": "Get this value from https://discord.com/developers/applications.",
        "required": true
      },
      "DISCORD_PREFIX": {
        "description": "Input here the prefix for your bot.",
        "value": "$",
        "require": true
      },
      "DISCORD_OWNER": {
        "description": "Input here your ID as the bot owner.",
        "required": true
      },
      "IMDB_KEY": {
        "description": "Get this value from https://imdb.com.",
        "required": true
      },
      "INVITE_CODE": {
        "description": "Input here the guild invite code.",
        "required": true
      },
      "INVITE_PERMISSION": {
        "description": "Input here the invite permission.",
        "value": "2013654135",
        "required": true
      },
      "INVITE_SCOPE": {
        "description": "Input here the invite scope.",
        "value": "bot,applications.commands",
        "required": true
      },
      "MONGO_URI": {
        "description": "MongoDB URI. Get from https://mongodb.com/",
        "required": true
      },
      "OPEN_WEATHER_ID": {
        "description": "Get your own APPID (API key)from https://api.openweathermap.org/data/2.5/weather",
        "required": true
      },
      "SPOTIFY_CLIENT_ID": {
        "description": "Spotify API Key. Get from https://developer.spotify.com/documentation/general/guides/app-settings/",
        "required": true
      },
      "SPOTIFY_CLIENT_SECRET": {
        "description": "Spotify API Key. Get from https://developer.spotify.com/documentation/general/guides/app-settings/",
        "required": true
      },
      "WEBHOOK_ID": {
        "description": "Input here the webhook id.",
        "require": true
      },
      "WEBHOOK_TOKEN": {
        "description": "Input here the webhook token.",
        "require": true
      },
      "YOUTUBE_KEY": {
        "description": "YouTube Data API Key. Get from https://developers.google.com/youtube/v3/getting-started",
        "required": true
      }
    }
}
