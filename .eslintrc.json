{
    "extends": "tesseract",
    "rules": {
        "complexity": ["error", { "max": 30 }],
        "consistent-return": "off",
        "func-names": ["error", "never"],
        "max-len": ["error", { "code": 320 }],
        "new-cap": ["error", { "newIsCapExceptionPattern": "^this\\.." }],
        "no-process-env": "off",
        "no-shadow": ["error", { "allow": ["msg", "button"] }]
    }
}